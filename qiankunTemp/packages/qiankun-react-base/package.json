{
  "name": "qiankun-react-base",
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "@umijs/plugin-qiankun": "^2.24.1",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.3.2",
    "antd": "^3.19.5",
    "braft-editor": "^2.3.9",
    "dynamic-antd-theme-yfhx": "^0.1.1",
    "qiankun": "^2.3.2",
    "qiankun-react-view": "^0.0.1",
    "react": "^16.12.0",
    "react-activation": "^0.9.1",
    "react-dom": "^16.12.0",
    "react-resizable": "^1.11.1",
    "redux": "^4.0.5",
    "umi": "^3.3.2",
    "use-merge-value": "^1.0.2",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1"
  }
}
